<launch>

    <param name="robot_description" command="$(find xacro)/xacro $(find car_description)/urdf/model00_car.xacro" />
    <!-- 启动 gazebo -->
    <include file="$(find gazebo_ros)/launch/empty_world.launch" >
        <arg name="world_name" value="$(find robot_gazebo)/config/gmapping_test.world" />
    </include>

    <!-- 在 gazebo 中显示机器人模型 -->
    <node pkg="gazebo_ros" type="spawn_model" name="model" args="-urdf -model mycar -param robot_description"  />    
    <!-- <include file="$(find simbot)/launch/robot_rviz.launch" /> -->

</launch>